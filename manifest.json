{
  "manifest_version": 2,
  "name": "First extension",
  "version": "0.1.0",
  "description": "Change spans color extension",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["script.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "images/fs.png",
    "default_popup": "popup/popup.html",
    "default_title": "A popup is here"
  }
}